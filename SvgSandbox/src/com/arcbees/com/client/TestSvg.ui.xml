<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style>
    
  </ui:style>
  <g:HTMLPanel>
  	<g:HorizontalPanel>
  		<g:PushButton text="Draw SVG" ui:field="drawSvg"/>
        <g:PushButton text="Draw Canvas" ui:field="drawCanvas"/>
  		<g:Cell verticalAlignment="ALIGN_MIDDLE">
  			<g:IntegerBox width="49px" text="1" ui:field="times"/>
  		</g:Cell>
      <g:HTML>&nbsp;&nbsp;&nbsp;</g:HTML>
      <g:PushButton text="Clear Panel" ui:field="clear"/>
      <g:HTML>&nbsp;&nbsp;&nbsp;</g:HTML>
      <g:PushButton text="Clear Svg" ui:field="clearSvg"/>
  	</g:HorizontalPanel>
      
      <g:SimplePanel ui:field="panel"/>
      
  </g:HTMLPanel>
</ui:UiBinder> 